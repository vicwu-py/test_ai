    "\n",
    "if chines == 100:\n",
    "    if math == 100:\n",
    "       bouns = 1000\n",
    "    else:\n",
    "        bouns = 500\n",
    "else:\n",
    "    if math == 100 :\n",
    "       bouns=500\n",
    "\n",
    "print(f\"獎金{bouns}\")\n",
    "    "
